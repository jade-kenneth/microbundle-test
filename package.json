{
  "name": "test-microbundle",
  "version": "0.0.5",
  "description": "multiple entry using microbundle",
  "main": "index.js",
  "type": "module",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "microbundle --minify src/*.ts --jsx  React.createElement --jsxFragment React.Fragment -o dist/"
  },
  "exports": {
    "./button": "./dist/button.modern.js",
    "./alert": "./dist/test-microbundle.modern.js"
  },
  "typesVersions": {
    "*": {
      "button": [
        "dist/button.d.ts"
      ],
      "alert": [
        "dist/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "jade kenneth darunday",
  "license": "ISC",
  "devDependencies": {
    "microbundle": "^0.15.1"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.1",
    "lerna": "^6.5.1"
  },
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jade-kenneth/microbundle-test.git"
  },
  "bugs": {
    "url": "https://github.com/jade-kenneth/microbundle-test/issues"
  },
  "homepage": "https://github.com/jade-kenneth/microbundle-test#readme"
}
